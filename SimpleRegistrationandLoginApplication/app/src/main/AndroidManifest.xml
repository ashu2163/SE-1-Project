<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.squirrel.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.squirrel.application.UpdateProfileActivity"></activity>
        <activity
            android:name="com.squirrel.application.LoginActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.squirrel.application.ManagerHomeActivity" />
        <activity android:name="com.squirrel.application.UserHomeActivity" />
        <activity android:name="com.squirrel.application.OperatorHomeActivity" />
        <activity android:name="com.squirrel.application.AssignActivity" />
        <activity android:name="com.squirrel.application.ConfirmEmailActivity" />
        <activity android:name="com.squirrel.application.ForgotPasswordActivity" />
        <activity android:name="com.squirrel.application.RegistrationActivity" />
        <activity android:name="com.squirrel.application.SelectedVehicleInventory" />
        <activity android:name="com.squirrel.application.CartDetailsActivity" />
        <activity android:name="com.squirrel.application.ViewOperatorInventory" />
        <activity android:name="com.squirrel.application.ViewManagerInventory" />
        <activity android:name="com.squirrel.application.ViewManagerVehicle" />
        <activity android:name="com.squirrel.application.ViewManagerLocation" />
        <activity android:name="com.squirrel.application.ViewManagerRevenue" />
        <activity android:name="com.squirrel.application.EditlocationActivity" />
    </application>

</manifest>